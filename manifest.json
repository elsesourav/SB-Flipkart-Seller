{
   "name": "Seller Automation ES",
   "version": "1.2.0",
   "description": "A tool for online sellers, automating listings and mapping to streamline e-commerce operations. (Copyright Â© SouravBarui2024).",
   "manifest_version": 3,
   "permissions": [
      "scripting",
      "storage",
      "tabs",
      "activeTab",
      "unlimitedStorage",
      "declarativeNetRequest",
      "declarativeNetRequestFeedback",
      "webRequest",
      "declarativeNetRequestWithHostAccess"
   ],
   "host_permissions": ["*://*/*"],
   "background": {
      "service_worker": "./backgroundScripts/background.js"
   },
   "content_scripts": [
      {
         "matches": ["*://*/*"],
         "js": ["./utils.js", "./contentScripts/html2canvas.js", "./querySelector.js", "./contentScripts/style.js", "./contentScripts/nano.fun.js", "./contentScripts/small.fun.js", "./contentScripts/content.js"]
      }
   ],
   "web_accessible_resources": [
      {
         "resources": ["assets/*", "css/*"],
         "matches": ["*://*/*"]
      }
   ],
   "action": {
      "default_icon": {
         "16": "assets/icons/icon.png",
         "24": "assets/icons/icon.png",
         "32": "assets/icons/icon.png"
      },
      "default_title": "Testing",
      "default_popup": "./popup/popup.html"
   },
   "icons": {
      "16": "assets/icons/icon.png",
      "48": "assets/icons/icon.png",
      "128": "assets/icons/icon.png"
   }
}
